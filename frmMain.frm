Private Sub LoadShops(Optional bTrainersOnly As Boolean)
On Error GoTo error:
Dim oLI As ListItem

lvShops.ListItems.clear

tabShops.MoveFirst
Do Until tabShops.EOF
    If bOnlyInGame And tabShops.Fields("In Game") = 0 Then GoTo skip:
    If bTrainersOnly Then
        If tabShops.Fields("ShopType") <> 8 Then GoTo skip:
        If chkGlobalFilter.Value = 1 Then
            If cmbGlobalClass(1).ListIndex > 0 Then
                If Not tabShops.Fields("ClassRest") = 0 And Not tabShops.Fields("ClassRest") = cmbGlobalClass(1).ItemData(cmbGlobalClass(1).ListIndex) Then GoTo skip:
            End If
            If Val(txtGlobalLevel(0).Text) > 0 Then
                If tabShops.Fields("MinLVL") > Val(txtGlobalLevel(0).Text) + 1 Or tabShops.Fields("MaxLVL") <= Val(txtGlobalLevel(0).Text) Then GoTo skip:
            End If
        End If
    End If
    
    Call AddShop2LV(lvShops)

skip:
    tabShops.MoveNext
Loop

bKeepSortOrder = True
Call lvShops_ColumnClick(lvShops.ColumnHeaders(2))

If lvShops.ListItems.Count >= 1 Then Call lvShops_ItemClick(lvShops.ListItems(1))

lvShops.Refresh

Set oLI = Nothing

Exit Sub
error:
HandleError
Set oLI = Nothing

End Sub

Private Sub LoadSpells()
On Error GoTo error:
Dim oLI As ListItem

If bDebugExecTime Then
    Dim nOverallExecStart As Long, nOverallExecEnd As Long, nOverallExecElapsed As Long
    'Dim nTimedExecStart As Long, nTimedExecEnd As Long, nTimedExecElapsed As Long
    nOverallExecStart = GetTickCount()
End If

If tabSpells.RecordCount = 0 Then Exit Sub
tabSpells.MoveFirst

'num
'name
'Short
'Magery
'Level
'mana
'difficulty
Call SetupCharBless
lvSpells.ListItems.clear

DoEvents
Do Until tabSpells.EOF
    
    'If bOnlyLearnable And tabSpells.Fields("Learnable") = 0 Then GoTo skip:
    If bOnlyInGame Then
        If tabSpells.Fields("Learnable") = 0 And Len(tabSpells.Fields("Learned From")) <= 1 And Len(tabSpells.Fields("Casted By")) <= 1 _
        And (tabSpells.Fields("Magery") <> 5 Or (tabSpells.Fields("Magery") = 5 And tabSpells.Fields("ReqLevel") < 1)) Then
            If nNMRVer >= 1.8 Then
                If Len(tabSpells.Fields("Classes")) <= 1 Then GoTo skip:
            Else
                GoTo skip:
            End If
        End If
    End If
    
    Call AddSpell2LV(lvSpells, True)

skip:
    tabSpells.MoveNext
    'DoEvents
Loop

bKeepSortOrder = True
Call lvSpells_ColumnClick(lvSpells.ColumnHeaders(nLastSpellSort))

If lvSpells.ListItems.Count >= 1 Then Call lvSpells_ItemClick(lvSpells.ListItems(1))

lvSpells.Refresh

out:
If bDebugExecTime Then 'CALCULATE FINAL EXEC RECORDING
    nOverallExecEnd = GetTickCount(): nOverallExecElapsed = nOverallExecEnd - nOverallExecStart
    sDebugExecReport = AutoAppend("LoadSpells/OVERALL: " & Round(nOverallExecElapsed / 1000, 2) & "s", sDebugExecReport, vbCrLf)
End If
On Error Resume Next
Set oLI = Nothing
Exit Sub
error:
HandleError ("LoadSpells")
Resume out:
End Sub


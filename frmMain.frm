Private Sub FindNegates(ByRef nSpells() As Long, ByRef nAbils() As Long, Optional ByVal nLevel As Long)
Dim nInterval As Integer, x As Long, bMatch As Boolean, oLI As ListItem, i As Integer ', y As Long
Dim tSpellMinMax As SpellMinMaxDur, nAbilValue As Long
On Error GoTo error:

If nSpells(0) = 0 And nAbils(0) = 0 Then Exit Sub

Me.Enabled = False
bMapCancelFind = False
Load frmProgressBar
Call frmProgressBar.SetRange((tabItems.RecordCount + tabSpells.RecordCount) / 5)
frmProgressBar.ProgressBar.Value = 0
frmProgressBar.lblCaption.Caption = "Searching Items..."
Set frmProgressBar.objFormOwner = Me

DoEvents
frmProgressBar.Show vbModeless, Me
DoEvents

Call frmResults.SetupResultsWindow(False, Me)

tabItems.Index = "pkItems"
tabItems.MoveFirst

nInterval = 1
Do Until tabItems.EOF Or bMapCancelFind
    If bOnlyInGame And tabItems.Fields("In Game") = 0 Then GoTo skip_item:
    
    For x = 0 To 9
        If tabItems.Fields("NegateSpell-" & x) > 0 Then
            For i = 0 To UBound(nSpells())
                If tabItems.Fields("NegateSpell-" & x) = nSpells(i) Then GoTo add_item:
            Next i
        End If
    Next x
    
    For x = 0 To 19
        If tabItems.Fields("Abil-" & x) > 0 Then
            nAbilValue = tabItems.Fields("AbilVal-" & x)
            Select Case tabItems.Fields("Abil-" & x)
                Case 73, 124: 'dispellabill, negateabil
                    For i = 0 To UBound(nAbils())
                        If nAbilValue > 0 And nAbilValue = nAbils(i) Then GoTo add_item:
                    Next i
            
                Case 122, 153: 'removespell, killspell
                    For i = 0 To UBound(nSpells())
                        If nAbilValue > 0 And nAbilValue = nSpells(i) Then GoTo add_item:
                    Next i
                
                Case 139: 'spellimmu
                    If nAbilValue > 0 And nLevel > 0 And nLevel < nAbilValue Then GoTo add_item:
                    
            End Select
        End If
    Next x
    
    GoTo skip_item:
    
add_item:
    Set oLI = frmResults.lvResults.ListItems.Add()
    oLI.Text = "Item: " & tabItems.Fields("Name") & " (" & tabItems.Fields("Number") & ")"
    oLI.Tag = tabItems.Fields("Number")
    Set oLI = Nothing
    GoTo skip_item:
    
skip_item:
    If nInterval > 5 Then
        Call frmProgressBar.IncreaseProgress
        nInterval = 1
    Else
        nInterval = nInterval + 1
    End If
    
    tabItems.MoveNext
    DoEvents
Loop

DoEvents
If bMapCancelFind Then GoTo stop_search:

frmProgressBar.lblCaption.Caption = "Searching Spells..."
DoEvents

tabSpells.Index = "pkSpells"
tabSpells.MoveFirst

nInterval = 1
Do Until tabSpells.EOF Or bMapCancelFind
    bMatch = False
'    If tabSpells.Fields("Number") = 87 Then
'        Debug.Print tabSpells.Fields("Number")
'    End If
    If bOnlyInGame And Len(tabSpells.Fields("Casted By")) < 3 And tabSpells.Fields("Learnable") = 0 Then GoTo skip_spell:
    
    tSpellMinMax = GetCurrentSpellMinMax(IIf(nLevel > 0, True, False), nLevel, True)
    
    For x = 0 To 9
        If tabSpells.Fields("Abil-" & x) > 0 Then
            nAbilValue = tabSpells.Fields("AbilVal-" & x)
            Select Case tabSpells.Fields("Abil-" & x)
                Case 73, 124: 'dispellabill, negateabil
                    For i = 0 To UBound(nAbils())
                        If nAbilValue > 0 And nAbilValue = nAbils(i) Then GoTo add_spell:
                    Next i
            
                Case 122, 153: 'removespell, killspell
                    For i = 0 To UBound(nSpells())
                        If nAbilValue > 0 And nAbilValue = nSpells(i) Then GoTo add_spell:
                    Next i
                
                Case 139: 'spellimmu
                    If nAbilValue > 0 And nLevel > 0 And nLevel < nAbilValue Then
                        If tabSpells.Fields("Learnable") = 1 Then GoTo add_spell:
                        If InStr(1, tabSpells.Fields("Casted By"), "Item #", vbTextCompare) > 1 Then GoTo add_spell:
                    End If
                    
            End Select
        End If
    Next x
    
    GoTo skip_spell:
    
add_spell:
    Set oLI = frmResults.lvResults.ListItems.Add()
    oLI.Text = "Spell: " & tabSpells.Fields("Name") & " (" & tabSpells.Fields("Number") & ")"
    oLI.Tag = tabSpells.Fields("Number")
    Set oLI = Nothing
    GoTo skip_spell:
    
skip_spell:
    If nInterval > 5 Then
        Call frmProgressBar.IncreaseProgress
        nInterval = 1
    Else
        nInterval = nInterval + 1
    End If
    
    tabSpells.MoveNext
    DoEvents
Loop
stop_search:
Unload frmProgressBar

If frmResults.lvResults.ListItems.Count > 0 Then
    frmResults.lblCaption = "Spell/Ability Negated By:"
    frmResults.Show vbModeless, IIf(bNoAlwaysOnTop, Nothing, Me)
Else
    Unload frmResults
    DoEvents
    MsgBox "Nothing.", vbOKOnly + vbInformation
End If

out:
Unload frmProgressBar
Me.Enabled = True
If FormIsLoaded("frmResults") Then
    frmResults.SetFocus
Else
    frmMain.SetFocus
End If

On Error Resume Next
Exit Sub
error:
Call HandleError("FindNegates")
Resume out:
End Sub


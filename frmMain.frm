        For x = 1 To Len(sFile)
            If InStr(x, sFile, "\") > 0 Then
                y = InStr(x, sFile, "\") + 1
                x = y
            End If
        Next x
        If y = 0 Then y = 1
        sFileTitle = Mid(sFile, y)
    End If
End If

If Not bDontShowLoadForm Then
    Me.Enabled = False
    frmLoadChar.Tag = "-1"

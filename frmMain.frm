Private Sub FindRoomWithDirections()
On Error GoTo error:
Dim x As Integer, nDir As Integer, nSearchMask As Integer, nRoomMask As Integer
Dim sLook As String, sFind As String, RoomExit As RoomExitType, nExitType As Integer
Dim oLI As ListItem, bExactMatch As Boolean

If tabRooms.RecordCount = 0 Then Exit Sub

Me.Enabled = False
Load frmPopUpOptions
Call frmPopUpOptions.ResetRoomFind
If nMapStartMap > 0 And nMapStartRoom > 0 Then
    frmPopUpOptions.txtRoomName.Text = GetRoomName(, nMapStartMap, nMapStartRoom, True)
End If
frmPopUpOptions.Tag = "-1"
frmPopUpOptions.Show vbModal, Me

If frmPopUpOptions.Tag <> "1" Then Exit Sub

If Len(Trim(frmPopUpOptions.txtRoomName.Text)) < 3 Then Exit Sub
For nDir = 0 To 9
    If frmPopUpOptions.cmdRoomFindDir(nDir).Tag = "1" Then nSearchMask = nSearchMask Or (2 ^ nDir)
Next nDir
If nSearchMask = 0 Then Exit Sub

sFind = Trim(frmPopUpOptions.txtRoomName.Text)
If frmPopUpOptions.optRoomFindMatch(1).Value = True Then bExactMatch = True

tabRooms.Index = "idxRooms"
tabRooms.MoveFirst

Me.Enabled = False
If FormIsLoaded("frmMap") Then frmMap.Enabled = False

bMapCancelFind = False
Load frmProgressBar
Call frmProgressBar.SetRange(tabRooms.RecordCount)
frmProgressBar.ProgressBar.Value = 1
frmProgressBar.lblCaption.Caption = "Searching Room Name + Exits ..."
Set frmProgressBar.objFormOwner = Me
DoEvents
frmProgressBar.Show vbModeless, Me
DoEvents

Call frmResults.SetupResultsWindow(False, Me)

Do Until tabRooms.EOF Or bMapCancelFind
    
    If bExactMatch Then
        If Not Trim(tabRooms.Fields("Name")) = sFind Then GoTo skip_room:
    Else
        If InStr(1, tabRooms.Fields("Name"), sFind, vbTextCompare) = 0 Then GoTo skip_room:
    End If
    
    nRoomMask = 0
    For x = 0 To 9
        Select Case x
            Case 0: sLook = "N"
            Case 1: sLook = "S"
            Case 2: sLook = "E"
            Case 3: sLook = "W"
            Case 4: sLook = "NE"
            Case 5: sLook = "NW"
            Case 6: sLook = "SE"
            Case 7: sLook = "SW"
            Case 8: sLook = "U"
            Case 9: sLook = "D"
        End Select
        
        nExitType = 0
        If Left(tabRooms.Fields(sLook), 6) = "Action" Then
            GoTo skip_exit:
            
        ElseIf Not Val(tabRooms.Fields(sLook)) = 0 Then
            RoomExit = ExtractMapRoom(tabRooms.Fields(sLook))
            
            If Len(RoomExit.ExitType) > 2 Then
                Select Case Left(RoomExit.ExitType, 5)
                    Case "(Key:": nExitType = 2
                    Case "(Item": nExitType = 3
                    Case "(Toll": nExitType = 4
                    Case "(Hidd": nExitType = 6
                    Case "(Door": nExitType = 7
                    Case "(Trap": nExitType = 9
                    Case "(Text": nExitType = 10
                    Case "(Gate": nExitType = 11
                    Case "Actio": nExitType = 12
                    Case "(Clas": nExitType = 13
                    Case "(Race": nExitType = 14
                    Case "(Leve": nExitType = 15
                    Case "(Time": nExitType = 16
                    Case "(Tick": nExitType = 17
                    Case "(Max ": nExitType = 18
                    Case "(Bloc": nExitType = 19
                    Case "(Alig": nExitType = 20
                    Case "(Dela": nExitType = 21
                    Case "(Cast": nExitType = 22 '" (Cast: " & "pre-" & nNUM & ", post-" & nNUM & ")"
                    Case "(Abil": nExitType = 23
                    Case "(Spel": nExitType = 24 '" (Spell Trap: " & nNUM & ")"
                End Select
            End If
            If Not RoomExit.Map = tabRooms.Fields("Map Number") Then nExitType = 8 'map change

            Select Case nExitType
                Case 6, 10, 12, 16: '6-hidden, 10-text, 12-remote, 16-timed
                    GoTo skip_exit:
                Case Else:
                    nRoomMask = nRoomMask Or (2 ^ x)
            End Select
        End If
skip_exit:
    Next x
    
    If nRoomMask = nSearchMask Then
        If frmResults.lvResults.ListItems.Count > 100 Then GoTo maxlimit:
        'Debug.Print tabRooms.Fields("Map Number") & "/" & tabRooms.Fields("Room Number")
        Set oLI = frmResults.lvResults.ListItems.Add()
        oLI.Text = "Room: " & tabRooms.Fields("Name") & " (" & tabRooms.Fields("Map Number") & "/" & tabRooms.Fields("Room Number") & ")"
        oLI.Tag = tabRooms.Fields("Map Number") & "/" & tabRooms.Fields("Room Number")
        Set oLI = Nothing
    End If
    
skip_room:
    Call frmProgressBar.IncreaseProgress
    tabRooms.MoveNext
    DoEvents
Loop

GoTo finish:

maxlimit:
MsgBox "Over 100 rooms found, quitting search.", vbOKOnly + vbInformation

finish:
If frmResults.lvResults.ListItems.Count > 0 Then
    frmResults.lblCaption = "Matching rooms:"
    frmResults.Show vbModeless, IIf(bNoAlwaysOnTop, Nothing, Me)
Else
    Unload frmResults
    DoEvents
    MsgBox "Nothing Found.", vbOKOnly + vbInformation
End If

out:
On Error Resume Next
If FormIsLoaded("frmResults") Then
    frmResults.SetFocus
Else
    frmMain.SetFocus
End If
Exit Sub
error:
Call HandleError("FindRoomWithDirections")
Resume out:
End Sub



        If lvSpellCompare.ListItems.Count > 0 Then
            Call ClearListViewSelections(lvSpellCompare)
            Call ProcessListViewClick(lvSpellCompare.ListItems(1), txtSpellCompareDetail, lvSpellCompareLoc)

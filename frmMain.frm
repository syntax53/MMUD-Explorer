Private Sub cmdRoomsButtons_Click(Index As Integer)
On Error GoTo error:
Dim x As Integer

Select Case Index
    Case 0: 'MapOpenExternal
        'If FormIsLoaded("frmMap") Then MsgBox "Already loaded"
        Load frmMap
        If nMapStartMap > 0 And nMapStartRoom > 0 Then
            Call frmMap.MapStartMapping(nMapStartMap, nMapStartRoom)
        End If
        frmMap.Show
        Me.WindowState = vbMinimized
        frmMap.SetFocus
        
    Case 1: 'ViewMapLegend
        If cmdRoomsButtons(1).Tag = "1" Then
            Unload frmMapLegend
            cmdRoomsButtons(1).Tag = "0"
        Else
            cmdRoomsButtons(1).Tag = "1"
            frmMapLegend.Show False, Me
            Set frmMapLegend.objFormOwner = Me
        End If
        
    Case 2: 'q
        MsgBox "Mob Damage in tooltip fill follow settings on the monster tab for vs party or vs char. Calculated damage will be utilized where available. Do a full calculate of all mobs for best results." _
            & vbCrLf & vbCrLf & "The [B] button (blocks) allows you to shift+right-click on any block to set the center position of the grid." _
            & vbCrLf & vbCrLf & "Show all exits in tooltip will display each exit and their destination in the tooltip.  This can help in areas where there is overlap, like portals." _
            & vbCrLf & vbCrLf & "Allow duplicates will allow a room to be drawn twice on the same map." _
            & vbCrLf & vbCrLf & "Allow overwrite allows a cell to be overwritten as the map draws. " _
            & "This can help on maps that overlap their own grids (i.e. stairs). " _
            & vbCrLf & vbCrLf & "Having both of these options turned on for some maps can produce... interesting results.", vbInformation
        
    Case 3: 'MapShowUnused
        If cmdRoomsButtons(3).Tag = 0 Then
            For x = 1 To 690
                lblRoomCell(x).Visible = True
            Next
            cmdRoomsButtons(3).Tag = 1
        Else
            For x = 1 To 690
                If CellRoom(x, 1) = 0 Then lblRoomCell(x).Visible = False
            Next
            cmdRoomsButtons(3).Tag = 0
        End If
        
    Case 4: 'Go Back
        If nMap_iGoBack >= 20 Then
            nMap_iGoBack = 1
        Else
            nMap_iGoBack = nMap_iGoBack + 1
        End If
again:
        If nMapLastMap(nMap_iGoBack) > 0 And nMapLastRoom(nMap_iGoBack) > 0 Then
            Call MapStartMapping(nMapLastMap(nMap_iGoBack), nMapLastRoom(nMap_iGoBack))
        ElseIf nMap_iGoBack > 1 Then
            nMap_iGoBack = 1
            GoTo again:
        End If
        
    Case 5: 'MegaMUDPathing
        txtMapMove.Text = "Disabled while megamud pathing window is open."
        txtMapMove.Enabled = False
        frmMegaMUDPath.Show
        frmMegaMUDPath.SetFocus
        
End Select

out:
On Error Resume Next
Exit Sub
error:
Call HandleError("cmdRoomsButtons_Click")
Resume out:
End Sub




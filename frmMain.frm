For Each oLI In lvShopDetail.ListItems
    str = str & oLI.Text
    If Len(oLI.Text) <= 6 Then str = str & String(7 - Len(oLI.Text), " ")
    
    x = 1
    For Each oLSI In oLI.ListSubItems
    
        If x = 1 Then
            str = str & Left(oLSI, 29)
            str = str & String(30 - Len(oLSI), " ")
        Else
            str = str & oLSI
            If Len(oLSI) < 7 Then str = str & String(7 - Len(oLSI), " ")
        End If
        
        x = x + 1
    Next
    str = str & vbCrLf
Next

done:
If Not str = "" Then
    'Clipboard.clear
    'Clipboard.SetText str
    Call SetClipboardText(str)
End If

Set oLI = Nothing
Set oLSI = Nothing
Set oCH = Nothing

Exit Sub
error:
HandleError
Set oLI = Nothing
Set oLSI = Nothing
Set oCH = Nothing
End Sub

